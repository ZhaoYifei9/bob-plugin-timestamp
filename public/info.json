{
  "identifier": "yuzhang.timestamp",
  "version": "0.0.3",
  "category": "translate",
  "name": "bob-plugin-timestamp",
  "summary": "时间戳转换",
  "icon": "",
  "author": "yuzhang9804@gmail.com",
  "homepage": "https://github.com/yuzhang9804/bob-plugin-timestamp",
  "appcast": "https://raw.githubusercontent.com/yuzhang9804/bob-plugin-timestamp/main/appcast.json",
  "minBobVersion": "0.5.0",
  "options": [
    {
      "identifier": "unit",
      "type": "menu",
      "title": "时间戳单位",
      "defaultValue": "s",
      "menuValues": [
        {
          "title": "毫秒",
          "value": "ms"
        },
        {
          "title": "秒",
          "value": "s"
        }
      ]
    },
    {
      "identifier": "format",
      "type": "text",
      "title": "时间格式",
      "defaultValue": "YYYY-MM-DD HH:mm:ss",
      "textConfig": {
        "type": "visible",
        "placeholderText": "时间格式"
      }
    },
    {
      "identifier": "timezone",
      "type": "text",
      "title": "时区",
      "defaultValue": "Asia/Shanghai",
      "textConfig": {
        "type": "visible",
        "placeholderText": "时区"
      }
    }
  ]
}
